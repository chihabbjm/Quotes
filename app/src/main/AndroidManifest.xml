<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.quotes.quotes">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/glasses"
        android:label="@string/app_name"
        android:logo="@mipmap/quotes_icon"
        android:roundIcon="@mipmap/glasses"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splash_screen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PublishPost"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginAdmin"
            android:screenOrientation="portrait" />
        <activity
            android:name=".costomizetosat"
            android:screenOrientation="portrait" />
        <activity android:name=".List_Published_Post"></activity>
    </application>

</manifest>